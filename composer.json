{
    "name": "jmiy/business-hyperf-3-1",
    "type": "library",
    "license": "MIT",
    "minimum-stability": "dev",
    "require": {
        "php": ">=8.0",
        "hyperf/async-queue": "^3.1",
        "hyperf/constants": "^3.1",
        "hyperf/signal": "^3.1",
        "hyperf/snowflake": "^3.1",
        "hyperf/tracer": "^3.1",
        "hyperf/translation": "^3.1",
        "hyperf/validation": "^3.1",
        "jmiy/hyperf-ding-notice-3-1": "dev-main",
        "hyperf/grpc": "^3.1",
        "hyperf/grpc-client": "^3.1",
        "hyperf/grpc-server": "^3.1",
        "hyperf/rpc": "^3.1",
        "hyperf/rpc-client": "^3.1",
        "hyperf/rpc-multiplex": "^3.1",
        "hyperf/rpc-server": "^3.1",
        "hyperf/json-rpc": "^3.1",
        "hyperf/load-balancer": "^3.1",
        "jmiy/dtm-client": "^1.0@dev"
    },
    "autoload":{
        "psr-4":{
            "Business\\Hyperf\\":"src/"
        },
        "files": [
            "src/Functions.php",
            "src/Utils/Collection/Functions.php"
        ]
    },
    "description": "",
    "repositories": {
        "packagist": {
            "type": "composer",
            "url": "https://mirrors.aliyun.com/composer/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "HyperfTest\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "test": "phpunit -c phpunit.xml --colors=always",
        "analyse": "phpstan analyse --memory-limit 1024M -l 1 ./src",
        "cs-fix": "php-cs-fixer fix $1"
    },
    "extra": {
        "hyperf": {
            "config": "Business\\Hyperf\\ConfigProvider"
        }
    }
}
